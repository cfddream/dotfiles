checkSSHTunnel() {
  text="`curl --socks5 127.0.0.1:$SSHTUNNEL_PORT --connect-timeout 21 --max-time 21 --silent --url http://apple.com`"
  if [ "$text" = '' ]; then
    echo 1
  else
    echo 0
  fi
}
